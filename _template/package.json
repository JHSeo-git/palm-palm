{
  "name": "template-gatsby",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare-gatsby": "babel gatsby -d . -x .ts,.tsx --presets gatsby-package",
    "dev": "yarn prepare-gatsby && gatsby develop",
    "build": "yarn prepare-gatsby && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "gatsby": "next",
    "react": "next",
    "react-dom": "next"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.16.1",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "babel-preset-gatsby-package": "^2.1.0",
    "typescript": "4.4.4"
  }
}
